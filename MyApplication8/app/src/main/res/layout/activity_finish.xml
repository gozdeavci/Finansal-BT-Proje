<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.barbaros.raceyourwords.FinishActivity">

    <LinearLayout
        android:layout_width="150dp"

        android:layout_height="250dp"
        android:orientation="vertical"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="36dp"
        app:layout_constraintVertical_bias="0.498"
        android:id="@+id/linearLayout9">

        <TextView
            android:id="@+id/textView_bitti"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.35"
            android:text="Soru Sayisi"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="0dp"

            android:layout_weight="1.35"
            android:text="Yanlış Cevap Sayısı"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.35"

            android:text="Doğru Cevap Sayısı"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="0dp"

            android:layout_weight="1.35"
            android:text="Skorunuz"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="88dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Yarışma Bitti"
        android:textSize="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="150dp"
        android:layout_height="250dp"
        android:layout_marginTop="114dp"
        android:orientation="vertical"

        app:layout_constraintTop_toTopOf="@+id/textView3"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/linearLayout9"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp">

        <TextView
            android:id="@+id/tv_sorusayisi"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="59dp"
            android:layout_marginRight="59dp"

            android:layout_weight="1"
            android:text="Soru Sayısı"
            android:textSize="20dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_constraintBottom_creator="1"

            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <TextView
            android:id="@+id/textView_yc"
            android:layout_width="match_parent"
            android:layout_height="49dp"

            android:layout_weight="1"

            android:text="Soru Sayısı"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/textView_skor"
            app:layout_constraintLeft_toLeftOf="@+id/textView_dc"
            app:layout_constraintRight_toRightOf="@+id/textView_dc"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />

        <TextView
            android:id="@+id/textView_dc"
            android:layout_width="50dp"
            android:layout_height="49dp"
            android:layout_marginBottom="9dp"
            android:layout_weight="1"

            android:text="Soru Sayısı"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/textView_yc"
            app:layout_constraintLeft_toLeftOf="@+id/tv_sorusayisi"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <TextView
            android:id="@+id/textView_skor"
            android:layout_width="50dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:text="Soru Sayısı"

            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView_yc"

            app:layout_constraintRight_toRightOf="@+id/textView_yc"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />

    </LinearLayout>

    <Button
        android:id="@+id/button_anasayfa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Anasayfa"
        android:textColor="@color/textColorb"
        android:background="@color/colorPrimary"
        android:textSize="14sp"
       app:layout_constraintBottom_toBottomOf="parent"
        tools:text="Anasayfa" />
    <![CDATA[


    >

]]>
</android.support.constraint.ConstraintLayout>
